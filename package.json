{
  "name": "lamatomic",
  "version": "2.0.0",
  "description": "VueJS UI Components under Atomic Design Methodology",
  "author": "Lamacompta",
  "license": "Lamacompta Copyright",
  "private": true,
  "main": "dist/lamatomic.ssr.js",
  "browser": "dist/lamatomic.esm.js",
  "module": "dist/lamatomic.esm.js",
  "unpkg": "dist/lamatomic.min.js",
  "types": "lamatomic.d.ts",
  "files": [
    "dist/*",
    "lamatomic.d.ts",
    "src/**/*.vue"
  ],
  "scripts": {
    "dev": "vue-cli-service serve dev/serve.ts",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint src/components/**/*.{vue,scss} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "dependencies": {
    "v-calendar": "^1.0.8",
    "vue-carousel": "^0.18.0",
    "vue-typed-js": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-replace": "^2.3.2",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/addons": "^5.3.18",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/vue": "^5.3.18",
    "@types/storybook__vue": "^5.2.1",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-typescript": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "jest": "^26.1.0",
    "lint-staged": "^10.0.0",
    "minimist": "^1.2.5",
    "node-sass": "^4.14.1",
    "prettier": "^1.19.1",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^8.0.2",
    "storybook-addon-themes": "^5.4.1",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^7.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "vue": "^2.6.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.2",
    "vue-template-compiler": "^2.6.11",
    "vue-test-utils": "^1.0.0-beta.11"
  },
  "peerDependencies": {
    "vue": "^2.6.11"
  },
  "engines": {
    "node": ">=10"
  }
}
